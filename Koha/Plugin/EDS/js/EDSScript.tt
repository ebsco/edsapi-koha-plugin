delCookie = function (name) {
	var exp = new Date();
	exp.setTime(exp.getTime() - 1);
	if (parent.opener) {
		parent.opener.document.cookie = name + "=null; path=/; expires=" + exp.toGMTString();
	} else {
		document.cookie = name + "=null; path=/; expires=" + exp.toGMTString();
	}
}

