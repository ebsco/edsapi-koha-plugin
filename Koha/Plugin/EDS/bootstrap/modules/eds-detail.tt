                                                [% IF ( SYNDETICS_TO.p ) %]
                                                <li><b>[% SYNDETICS_TO.l %] [% SYNDETICS_TO.t %]</b>[% IF ( SYNDETICS_TO.p ) %] ([% SYNDETICS_TO.p %])[% END %]</li>
                                                [% END %]
                                            [% END %]
                                        </ul>
                                </div>
                            </div>
