                                    <h3>Author notes provided by Syndetics</h3>
                                    [% FOREACH SYNDETICS_ANOTE IN SYNDETICS_ANOTES %]
                                        [% IF ( SYNDETICS_ANOTE.content ) %]
                                            [% SYNDETICS_ANOTE.content %]
                                        [% END %]
                                    [% END %]
                                </div>
                            </div>
                        [% END # / IF SyndeticsReviews && SYNDETICS_REVIEWS %]
                    [% END # / IF SyndeticsEnabled %]

